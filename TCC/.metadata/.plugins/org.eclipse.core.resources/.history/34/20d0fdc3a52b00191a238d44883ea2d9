package com.java.socket;

public class SmartLamp {
	
	private String id;
	private String typeObject = "Room";
	private int luminosity;
	private String typeLuminosity = "Int";
	private String status;
	private String typeStatus = "String";
	
	
	public SmartLamp(String id, int luminosity, String status) {
		this.id = id;
		this.luminosity = luminosity;
		this.status = status;
	}
	
	
	public SmartLamp() {
		
	}


		
	public String getId() {
		return id;
	}


	public String getTypeObject() {
		return typeObject;
	}


	public int getLuminosity() {
		return luminosity;
	}


	public void setLuminosity(int luminosity) {
		this.luminosity = luminosity;
	}


	public String getTypeLuminosity() {
		return typeLuminosity;
	}


	public String getStatus() {
		return status;
	}


	public void setStatus(String status) {
		this.status = status;
	}


	public String getTypeStatus() {
		return typeStatus;
	}


	@Override
	public String toString() {
		return "SmartLamp { /n /t Luminosity: " + luminosity + " /n /t Status: " + status + "/n }";
	}


}
